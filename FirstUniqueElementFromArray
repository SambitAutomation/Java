class FirstUniqueElement
{
static void notRepeatingElement( int[] arr)
{

for(int i=0;i<arr.length;i++)
{
int count =0 ;

for(int j=0;j<arr.length;j++)
{
if(arr[i] == arr[j])
{	
count = count + 1;
}
}

if(count == 1)
{
System.out.println(arr[i]);
break;
}
}
}

public static void main(String[] args)
{

notRepeatingElement(new int[]{10,20,30,11,20,30,10,28,32});
}
}
